{
  "title": "WhatsApp Bulk Messaging Actor Dataset",
  "type": "object",
  "schemaVersion": 1,
  "description": "Schema for the dataset output of WhatsApp Bulk Messaging Actor. Each record represents a message send operation result.",
  "properties": {
    "success": {
      "title": "Success",
      "type": "boolean",
      "description": "Whether the operation was successful"
    },
    "sessionId": {
      "title": "Session ID",
      "type": "string",
      "description": "The WhatsApp session identifier used"
    },
    "status": {
      "title": "Status",
      "type": "string",
      "description": "Current status of the session"
    },
    "message": {
      "title": "Message",
      "type": "string",
      "description": "Human-readable message about the operation result"
    },
    "to": {
      "title": "Recipient",
      "type": "string",
      "description": "Recipient phone number"
    },
    "type": {
      "title": "Message Type",
      "type": "string",
      "description": "Type of message sent",
      "enum": ["text", "attachment"]
    },
    "action": {
      "title": "Action",
      "type": "string",
      "description": "The action that was performed",
      "enum": ["connect", "send", "sendBulk"]
    },
    "index": {
      "title": "Message Index",
      "type": "integer",
      "description": "Message index in bulk send operation (1-based)"
    },
    "error": {
      "title": "Error",
      "type": "string",
      "description": "Error message if operation failed"
    },
    "total": {
      "title": "Total Messages",
      "type": "integer",
      "description": "Total number of messages in bulk send operation"
    },
    "successful": {
      "title": "Successful Messages",
      "type": "integer",
      "description": "Number of successfully sent messages"
    },
    "failed": {
      "title": "Failed Messages",
      "type": "integer",
      "description": "Number of failed messages"
    },
    "results": {
      "title": "Results",
      "type": "array",
      "description": "Detailed results for each message in bulk send operation"
    },
    "qrCodeSaved": {
      "title": "QR Code Saved",
      "type": "boolean",
      "description": "Whether a QR code was generated and saved"
    }
  }
}

